<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="metadata">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Title" type="stringNormal" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Description" type="stringLong" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Discipline" type="optionsDiscipline" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Version" type="stringNormal" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Language" type="stringNormal" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Collected">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Start_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
							<xs:element name="End_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Covered_Geolocation_Place" type="stringNormal" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Covered_Period">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Start_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
							<xs:element name="End_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Tag" type="stringNormal" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Related_Datapackage" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Relation_Type" type="optionsRelationType" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="Properties">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Title" type="stringNormal" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="Persistent_Identifier">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Identifier_Scheme" type="optionsDatasetPersistentIdentifierScheme" minOccurs="0" maxOccurs="unbounded"/>
													<xs:element name="Identifier" type="stringNormal" minOccurs="0" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Retention_Period" type="xs:integer" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Retention_Justification" type="stringNormal" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Embargo_End_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Data_Classification" type="optionsDataClassification" minOccurs="0" maxOccurs="1"/>
				<xs:element name="Collection_Name" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                <xs:element name="Funding_Reference" minOccurs="0" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="Funder_Name" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                      <xs:element name="Properties">
                                            <xs:complexType>
                                              <xs:sequence>
                                                <xs:element name="Award_Number" type="stringNormal" minOccurs="0" maxOccurs="1"/>
                                              </xs:sequence>
                                            </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType> 
                                </xs:element>
				<xs:element name="Creator" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>	
							<xs:element name="Name"	type="stringNormal" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Properties">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Identifier" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Name_Identifier_Scheme" type="optionsPersonalPersistentIdentifierType" minOccurs="0" maxOccurs="1"/>
													<xs:element name="Name_Identifier" type="stringNormal" minOccurs="0" maxOccurs="1"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Affiliation" type="stringNormal" minOccurs="0" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Contributor" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>	
							<xs:element name="Name"	type="stringNormal" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Properties">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Contributor_Type" minOccurs="0" maxOccurs="1"/>
										<xs:element name="Identifier" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Name_Identifier_Scheme" type="optionsPersonalPersistentIdentifierType" minOccurs="0" maxOccurs="1"/>
													<xs:element name="Name_Identifier" type="stringNormal" minOccurs="0" maxOccurs="1"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Affiliation" type="stringNormal" minOccurs="0" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="License" type="optionsLicense" minOccurs="0" maxOccurs="1">
                                </xs:element>
				<xs:element name="Access_Restriction" type="optionsDatasetAccess" minOccurs="0" maxOccurs="1"/>
				<xs:element name="system" minOccurs="0" maxOccurs="1">
					<xs:complexType>
					  <xs:sequence>
						<xs:element name="Last_Modified_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Persistent_Identifier_Datapackage" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Identifier_Scheme" type="optionsDatasetPersistentIdentifierScheme" minOccurs="0" maxOccurs="1"/>
									<xs:element name="Identifier" type="stringNormal" minOccurs="0" maxOccurs="1"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="Publication_Date" type="xs:date" minOccurs="0" maxOccurs="1"/>
						<xs:element name="Open_Access_Link" type="xs:anyURI" minOccurs="0" maxOccurs="1"/>
						<xs:element name="License_URL" type="xs:anyURI" minOccurs="0" maxOccurs="1"/>
					   </xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


	<xs:simpleType name="stringURI">
	    <xs:restriction base="xs:anyURI">
		<xs:maxLength value="1024"/>
	    </xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="stringNormal">
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="stringLong">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2700"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="optionsYesNo">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Yes"/>
			<xs:enumeration value="No"/>
			<xs:enumeration value="N.A."/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="optionsDatasetAccess">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Open"/>
			<xs:enumeration value="Restricted to UU"/>
			<xs:enumeration value="Restricted to UU dpt"/>
			<xs:enumeration value="Restricted - contact I-lab"/>
			<xs:enumeration value="Closed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="optionsDataClassification">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Public"/>
			<xs:enumeration value="Basic"/>
			<xs:enumeration value="Sensitive"/>
			<xs:enumeration value="Critical"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="optionsPersonalPersistentIdentifierScheme">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ORCID"/>
			<xs:enumeration value="DAI"/>
			<xs:enumeration value="Author identifier (Scopus)"/>
			<xs:enumeration value="ResearcherID (Web of Science)"/>
			<xs:enumeration value="ISNI"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Source: https://schema.datacite.org/meta/kernel-3.1/doc/DataCite-MetadataKernel_v3.1.pdf -->
	<xs:simpleType name="optionsDatasetPersistentIdentifierScheme">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ARK"/>
			<xs:enumeration value="arXiv"/>
			<xs:enumeration value="bibcode"/>
			<xs:enumeration value="DOI"/>
			<xs:enumeration value="EAN13"/>
			<xs:enumeration value="EISSN"/>
			<xs:enumeration value="Handle"/>
			<xs:enumeration value="ISBN"/>
			<xs:enumeration value="ISSN"/>
			<xs:enumeration value="ISTC"/>
			<xs:enumeration value="LISSN"/>
			<xs:enumeration value="LSID"/>
			<xs:enumeration value="PMID"/>
			<xs:enumeration value="PURL"/>
			<xs:enumeration value="UPC"/>
			<xs:enumeration value="URL"/>
			<xs:enumeration value="URN"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Source: https://www.oecd.org/science/inno/38235147.pdf -->
